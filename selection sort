public class Main
{
    static void selectionsort (int[] arr){
        int  n = arr.size
        for(int i = 0; i<n; i++){
            
            int min_idx =i;
            
            for(int j = i+1; j<n; j++){
                
                if (arr[j] < arr[min_idx]){
                    
                    int min_idx = j;
                    
                }
            }
        }
        
    }
}
